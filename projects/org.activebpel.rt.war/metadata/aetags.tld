       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
       <name>property</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>SetProperty</name>
    <tag-class>org.activebpel.rt.war.tags.AeSetPropertyTag</tag-class>
