  <tag>
    <name>IfTrue</name>
    <tag-class>org.activebpel.rt.war.tags.AeIfTrueTag</tag-class>
    <body-content>JSP</body-content>
    <description> Process body if bean property evaluates to true </description>
    <attribute>
