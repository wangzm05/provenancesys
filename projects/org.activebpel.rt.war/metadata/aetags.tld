       <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
       <name>fromName</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
       <name>fromProperty</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>    
  </tag>

