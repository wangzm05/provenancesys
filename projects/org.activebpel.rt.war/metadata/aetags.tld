       <name>name</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
       <name>property</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
       <name>value</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
       <name>classType</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>IfPropertyNotInList</name>
    <tag-class>org.activebpel.rt.war.tags.AeIfPropertyNotInListTag</tag-class>
    <body-content>JSP</body-content>
    <description> Process body if the string representation of property value is not in the given
