       <name>property</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
       <name>value</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>


  <tag>
    <name>IfPropertyMatches</name>
    <tag-class>org.activebpel.rt.war.tags.AeIfPropertyMatchesTag</tag-class>
    <body-content>JSP</body-content>
    <description> Process body if the property value matches the expected value </description>
